# Nano Banana Pro API 配置
# 获取API密钥: https://kie.ai/api-key
# VITE_API_KEY=your_api_key_here

# API基础URL（通常不需要修改）
VITE_API_BASE_URL=https://api.kie.ai/api/v1/jobs

# 默认生成参数
VITE_DEFAULT_ASPECT_RATIO=1:1
VITE_DEFAULT_RESOLUTION=1K
VITE_DEFAULT_OUTPUT_FORMAT=png

# 开发服务器配置
VITE_DEV_SERVER_PORT=3000
VITE_DEV_SERVER_HOST=localhost

# 应用配置
VITE_APP_TITLE=魔法英文小报提示词生成器
VITE_APP_DESCRIPTION=轻松生成儿童识字小报的AI绘图提示词